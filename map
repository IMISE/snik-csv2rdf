mkdir -p heinrich/tmp 
tarql heinrich/main.tarql | rapper -i turtle - http://www.snik.eu/ontology/he/ > heinrich/tmp/main.nt
tarql heinrich/axiom.tarql | rapper -i turtle - http://www.snik.eu/ontology/he/  > heinrich/tmp/axiom.nt
tarql heinrich/property.tarql | rapper -i turtle  - http://www.snik.eu/ontology/he/ > heinrich/tmp/property.nt
tarql heinrich/instance.tarql | rapper -i turtle  - http://www.snik.eu/ontology/he/ > heinrich/tmp/instance.nt

cat prefix.ttl heinrich/tmp/*.nt | rapper -i turtle -o turtle - http://www.snik.eu/ontology/he/ > heinrich/all.ttl
